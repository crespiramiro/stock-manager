{
  "version": 2,
  "builds": [
    {
      "src": "frontend/stock-manager/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "frontend/stock-manager/build"
      }
    },
    {
      "src": "backend/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/$1"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/stock-manager/$1"
    }
  ]
}